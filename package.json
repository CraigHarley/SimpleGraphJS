{
  "name": "simplegraphjs",
  "version": "1.0.6",
  "description": "A very simple graph implementation for use in JS, using an adjacency matrix..",
  "main": "dist/Graph.js",
  "types": "./dist/Graph.d.ts",
  "keywords": [
    "graph",
    "breadth first search",
    "search",
    "typescript",
    "node",
    "javascript",
    "simple"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "prepublish": "jest",
    "asbuild:untouched": "asc Graph.ts -b dist/untouched.wasm -t dist/untouched.wat --sourceMap --validate --debug",
    "asbuild:optimized": "asc Graph.ts -b dist/optimized.wasm -t dist/optimized.wat --sourceMap --validate --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "author": "Craig Harley <craigharley2@googlemail.com>",
  "repository": "https://github.com/CraigHarley/SimpleGraphJS",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "coveralls": "^3.0.2",
    "jest": "^24.0.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.1"
  },
  "dependencies": {}
}